/* components/z-music/z-music.wxss */

.contenet {
  width: 100%;
  height: 100%;
  background-size: 100% 100% ;
  width: 100%;
  background-repeat: no-repeat;
  background-size: cover;
  -webkit-filter: blur(15px);
  -moz-filter: blur(15px);
  -o-filter: blur(15px);
  -ms-filter: blur(15px);
  filter: blur(15px);
  position: absolute;
  left: 0;
  top: 0;
}

scroll-view ::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
  display: none;
}
