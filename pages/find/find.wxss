.musiclist {
  padding: 30rpx;
}

.musiclist .title {
  font-size: 28rpx;
  margin-bottom: 20px;
  font-weight: 600;
}

z-musiclist {
}

/*隐藏滚动条*/

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

z-minimusic {
  position: fixed;
  right: 0rpx;
  background: white;
  border-radius: 40rpx 0rpx 0rpx 40rpx;
  border: 1px solid #aaa;
  box-shadow: 0px 3px 4px 0px rgba(134, 132, 132, 0.4);
  z-index: 99;
  top: 45%;
}
